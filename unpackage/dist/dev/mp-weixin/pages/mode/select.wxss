  position: relative;
}
.mode-btn__view .new-game__view text.data-v-c71aca02,
.mode-btn__view .observe__view text.data-v-c71aca02,
.mode-btn__view .review__view text.data-v-c71aca02 {
  z-index: 1;
  font-size: 40px;
  color: #fff;
  font-family: '仓耳渔阳体 W05';
}
.mode-btn__view .new-game__view.data-v-c71aca02:before,
.mode-btn__view .observe__view.data-v-c71aca02:before,
.mode-btn__view .review__view.data-v-c71aca02:before {
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  /* 插入空内容，必须要的样式 */
  content: '';
  /* 绝对定位，确保背景图起点在主层的0,0位置 */
  position: absolute;
  left: 0;
  top: 0;
  /*避免遮盖其他元素*/
  z-index: 0;
  -webkit-filter: opacity(38%);
  /*设置透明度   blur还可以改变照片的模糊度*/
  filter: opacity(38%);
}
.mode-btn__view .new-game__view.data-v-c71aca02:before {
  background-image: url(/static/image/mode/new-game.jpg);
}
.mode-btn__view .observe__view.data-v-c71aca02:before {
